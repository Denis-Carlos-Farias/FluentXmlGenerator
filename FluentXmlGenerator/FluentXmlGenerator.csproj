<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Fluent.Xml.Generator</PackageId>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Make your XML files easier with FluentXmlGenerator</Title>
    <Authors>Denis Carlos de Farias</Authors>
    <Description>New easy way to build you Xml files</Description>
    <RepositoryUrl>https://github.com/Denis-Carlos-Farias/FluentXmlGenerator</RepositoryUrl>
  </PropertyGroup>
</Project>
